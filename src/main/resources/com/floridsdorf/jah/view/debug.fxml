<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane fx:id="rootPane" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
            fx:mainController="com.floridsdorf.jah.mainController.viewControllers.DebugController">
    <children>
        <VBox alignment="CENTER" spacing="20.0">
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </padding>

            <Button onAction="#onHomeButtonClick" text="home"/>
            <Button onAction="#onLobbyButtonClick" text="lobby"/>
            <Button onAction="#onMainGameButtonClick" text="mainGame"/>
            <Button onAction="#onResultButtonClick" text="result"/>
            <Button onAction="#onVoteButtonClick" text="vote"/>
        </VBox>
    </children>
</AnchorPane>
